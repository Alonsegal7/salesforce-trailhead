      {
        source = file("salesforce/Records/LightningComponentBundle/subscriptionPickersComponent/subscriptionPickersComponent.html")
        filePath = "lwc/subscriptionPickersComponent/subscriptionPickersComponent.html"
      },
      {
        source = file("salesforce/Records/LightningComponentBundle/subscriptionPickersComponent/subscriptionPickersComponent.css")
        filePath = "lwc/subscriptionPickersComponent/subscriptionPickersComponent.css"
      },
    ]
  }
  targetConfigs = {
    targetConfig = [
      {
        targets = "lightningCommunity__Default"
        "property
" = {
          name = "recordId"
