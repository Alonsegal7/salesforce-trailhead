  salesforce.Text Response_Status_CP__c {
    apiName = "Activity.Response_Status_CP__c"
    description = "Chilipiper sync field - Track if an Event is Accepted, Declined, NotResponded, or Maybe/Tentative."
    externalId = false
    inlineHelpText = "Chilipiper sync field - Track if an Event is Accepted, Declined, NotResponded, or Maybe/Tentative."
    label = "Response_Status_CP"
