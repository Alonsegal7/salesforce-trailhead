  salesforce.Text Type_2__c {
    apiName = "Activity.Type_2__c"
    externalId = false
    label = "Type 2"
    length = 100
    _required = false
    unique = false
  }
  salesforce.Lookup User__c {
    apiName = "Activity.User__c"
    description = "Chilipiper sync field - Track which user booked a meeting"
    inlineHelpText = "Chilipiper sync field - Track which user booked a meeting"
    label = "User"
    referenceTo = [
      salesforce.User,
    ]
    _required = false
  }
  salesforce.Checkbox dafadf__c {
    apiName = "Activity.dafadf__c"
    defaultValue = false
    label = "dafadf"
  }
  salesforce.FormulaText local_time__c {
    apiName = "Activity.local_time__c"
    description = "(time_zone_offset__c = NULL) || (TEXT(time_zone_offset__c) = '') || ISBLANK(TEXT(time_zone_offset__c)))"
    externalId = false
    formula = '''
IF( isblank( time_zone_offset__c ),"",
LEFT(TEXT(TIMEVALUE(NOW() + (time_zone_offset__c/24))), 5)
)
'''
    label = "local time"
    _required = false
    unique = false
    _generated_dependencies = [
      salesforce.Activity.field.time_zone_offset__c,
    ]
  }
  salesforce.Number time_zone_offset__c {
    apiName = "Activity.time_zone_offset__c"
    externalId = false
    label = "time zone offset"
    precision = 18
    _required = false
    scale = 0
    unique = false
  }
}
