  salesforce.Text SMS_Response_CP__c {
    apiName = "Activity.SMS_Response_CP__c"
    description = "Chilipiper sync field - Track the Response of an SMS Message - only used if using Twilio SMS Integration*"
    externalId = false
    inlineHelpText = "Chilipiper sync field - Track the Response of an SMS Message - only used if using Twilio SMS Integration*"
    label = "SMS_Response_CP"
    length = 255
    _required = false
    unique = false
  }
  salesforce.Picklist Task_Time_Zone__c {
    apiName = "Activity.Task_Time_Zone__c"
    label = "Task Time Zone"
    _required = false
    valueSetName = salesforce.GlobalValueSet.instance.Time_Zones
    restricted = true
  }
  salesforce.FormulaText Team_Manager__c {
    apiName = "Activity.Team_Manager__c"
    externalId = false
    formula = "Owner:User.Team_Manager__c"
    formulaTreatBlanksAs = "BlankAsZero"
    label = "Team Manager"
    _required = false
    unique = false
  }
  salesforce.Time Time__c {
    apiName = "Activity.Time__c"
    label = "Time"
    _required = false
