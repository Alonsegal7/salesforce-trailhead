salesforce.ListView Account_MyAccounts {
  fullName = "Account.MyAccounts"
  columns = [
    "ACCOUNT.NAME",
    "ACCOUNT.ADDRESS1_STATE",
    "ACCOUNT.PHONE1",
    "ACCOUNT.TYPE",
    "CORE.USERS.ALIAS",
  ]
  filterScope = "Mine"
  filters = [
    {
      field = "ACCOUNT.RECORDTYPE"
      operation = "equals"
      value = "Account.Company"
    },
  ]
  label = "My Companies"
  sharedTo = {
    roleAndSubordinatesInternal = "Head_of_Sales"
  }
  _parent = [
    salesforce.Account,
  ]
}
