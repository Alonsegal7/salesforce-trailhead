salesforce.ValidationRule Account_Cant_Change_Account_Owner_to_Partner {
  fullName = "Account.Cant_Change_Account_Owner_to_Partner"
  active = false
  errorConditionFormula = "Owner.Profile.Name ='Partner Community Custom'&&  $Profile.Name <> 'System Administrator'"
  errorDisplayField = "OwnerId"
  errorMessage = "For changing the account owner- please contact sales ops team"
  _parent = [
    salesforce.Account,
  ]
}
