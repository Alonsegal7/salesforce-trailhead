salesforce.WorkflowRule Lead_Update_distributed_to_first_field {
  fullName = "Lead.Update %27distributed to first%27 field"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Lead_Update_first_owner_name
      type = "FieldUpdate"
    },
  ]
  active = true
  criteriaItems = [
    {
      field = salesforce.Lead.field.OwnerId
      operation = "notEqual"
    },
  ]
  triggerType = "onCreateOnly"
  _parent = [
    salesforce.Lead,
  ]
}
