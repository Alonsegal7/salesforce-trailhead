salesforce.WorkflowRule Lead_Lead_Set_Distributed_Score_if_Empty {
  fullName = "Lead.Lead - Set Distributed Score if Empty"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Lead_Lead_Set_Distributed_Score_if_Empty
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = '''
(isnew()||ischanged( Current_score__c ))&&
isblank( Distributed_score__c )
'''
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Lead,
  ]
}
