salesforce.WorkflowRule Lead_Update_first_lead_source {
  fullName = "Lead.Update first lead source"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Lead_Update_first_lead_source
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = '''
(isnew()||ischanged( LeadSource ))&&
isblank(First_Lead_Source__c )&&
not ispickval(LeadSource,'')
'''
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Lead,
  ]
}
