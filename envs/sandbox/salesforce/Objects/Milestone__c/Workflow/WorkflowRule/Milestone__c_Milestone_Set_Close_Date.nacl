salesforce.WorkflowRule Milestone__c_Milestone_Set_Close_Date {
  fullName = "Milestone__c.Milestone - Set Close Date"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Milestone__c_Milestone_Set_Close_Date
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = '''
(isnew()||ischanged( Status__c ))&&
ispickval(Status__c,'Closed')
'''
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Milestone__c,
  ]
}
