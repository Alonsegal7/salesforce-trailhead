  salesforce.FormulaNumber GB_opp__c {
    apiName = "Opportunity.GB_opp__c"
    label = "Is Potential Green Bucket Opp"
    _required = false
    precision = 18
    scale = 0
    formula = '''
if(

or(

and(
RecordType.Name ='Partner Opportunity',
or( ispickval(Account.Account_Source_Type__c ,'Outbound'),
and( ispickval(Account.Account_Source_Type__c,'Inbound'), Green_Bucket_ARR__c >=5000))),

or(

and(

or(ispickval(Pricing_Version__c,"5"),ispickval(Pricing_Version__c,"6"),ispickval(Pricing_Version__c,"8"))
,lower(Expected_Plan_Period__c) ="yearly", Expected_ARR__c >=7000),
and(

or(ispickval(Pricing_Version__c,"6"),ispickval(Pricing_Version__c,"8"))
,or(and(lower(Expected_Plan_Period__c) ="yearly", Expected_Plan_Seats__c >=20, lower(Expected_Plan_Tier__c) ="enterprise"),
    and(lower(Expected_Plan_Period__c) ="yearly", Expected_Plan_Seats__c >=40, lower(Expected_Plan_Tier__c) ="pro"))),
    
and(
Expected_Plan_Seats__c >=25, lower(Expected_Plan_Tier__c) ="enterprise",lower(Expected_Plan_Period__c) ="yearly"),

and(
Expected_Plan_Seats__c >=50,or(lower(Expected_Plan_Period__c) ="yearly",Expected_Plan_Period__c ="two_yearly")))),1,0)
'''
    inlineHelpText = "Check if the opportunity is potential to be a green bucket Opp."
    createable = false
    updateable = false
    unique = false
    queryable = true
    description = '''
Internal - 
V5/V6/V8 with yearly plan with >7K ARR OR
V6/V8 with yearly Ent over 20/ Pro over 40 OR
Yearly Enterprise​ deals with >​25 seats OR
Yearly/2 Yearly plans with >50 seats

Partner - 
Outbound OR
Inbound with >5K ARR
'''
    externalId = false
    formulaTreatBlanksAs = "BlankAsZero"
    _generated_dependencies = [
      salesforce.Opportunity.field.Expected_Plan_Seats__c,
      salesforce.Opportunity.field.Expected_Plan_Tier__c,
      salesforce.Opportunity.field.Expected_Plan_Period__c,
      salesforce.Opportunity.field.Expected_ARR__c,
      salesforce.Opportunity.field.Green_Bucket_ARR__c,
      salesforce.Opportunity.field.Pricing_Version__c,
      salesforce.Account.field.Account_Source_Type__c,
    ]
  }
