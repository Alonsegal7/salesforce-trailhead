    apiName = "Opportunity.Out_of_Forecast__c"
    label = "Out of Forecast"
    formula = '''
if( RecordTypeId ='0121t000000Lucv',
OR(
iSPICKVAL( Sale__r.FinalSaleStatus__c , "Expected Complete") && ISPICKVAL(StageName,"Closed Won"),

(iSPICKVAL( Sale__r.FinalSaleStatus__c , "Complete") && ISPICKVAL(StageName,"Closed Won") &&  Expected_Plan_Period__c ="monthly")),

if( RecordTypeId ='0121t000000IQUb',
OR(
iSPICKVAL( Sale__r.FinalSaleStatus__c , "Expected Complete") && ISPICKVAL(StageName,"Closed Won"),

iSPICKVAL( Sale__r.FinalSaleStatus__c , "Complete") && iSPICKVAL(Account.Account_Source_Type__c,'Inbound') && ISPICKVAL(StageName,"Closed Won") && Amount<5000 &&  Account.ARR__c <5000),false))
