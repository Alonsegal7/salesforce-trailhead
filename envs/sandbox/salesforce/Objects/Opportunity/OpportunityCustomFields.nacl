    apiName = "Opportunity.Opportunity_Age__c"
    label = "Opportunity Age"
    _required = false
    precision = 18
    scale = 0
    formula = '''
IF(IsClosed,if(ispickval(StageName,"Closed Lost"), Closed_Lost_timestamp__c - DATEVALUE(CreatedDate), 
