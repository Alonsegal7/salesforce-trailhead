salesforce.ValidationRule Opportunity_Opp_Account_must_be_MA {
  fullName = "Opportunity.Opp_Account_must_be_MA"
  active = true
  errorConditionFormula = '''
(isnew()||ischanged( AccountId ))&& Account.RecordType.DeveloperName !='Monday_Account'&&/*MA*/
/*$Profile.Name!="System Administrator"&&*/
$Profile.Name!="Integration Administrator"
'''
  errorDisplayField = "AccountId"
  errorMessage = "Please select a Monday Account, not a Comapny"
  _parent = [
    salesforce.Opportunity,
  ]
}
