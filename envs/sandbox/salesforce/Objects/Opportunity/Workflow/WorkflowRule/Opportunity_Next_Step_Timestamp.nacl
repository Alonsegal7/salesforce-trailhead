salesforce.WorkflowRule Opportunity_Next_Step_Timestamp {
  fullName = "Opportunity.Next Step Timestamp"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Opportunity_Update_Next_Step_Timestamp
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = "NOT(ISBLANK(NextStep)) && ischanged(NextStep)"
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Opportunity,
  ]
}
