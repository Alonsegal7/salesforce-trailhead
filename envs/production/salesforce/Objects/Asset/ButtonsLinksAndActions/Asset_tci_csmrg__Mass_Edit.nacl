salesforce.WebLink Asset_tci_csmrg__Mass_Edit {
  fullName = "Asset.tci_csmrg__Mass_Edit"
  availability = "online"
  displayType = "massActionButton"
  linkType = "javascript"
  masterLabel = "Mass Edit"
  openType = "onClickJavaScript"
  protected = false
  requireRowSelection = true
  url = '''
{!REQUIRESCRIPT("/soap/ajax/19.0/connection.js")}

var records = {!GETRECORDIDS($ObjectType.Asset)};

if (records[0] == null) {

alert("Please select at least one Asset to edit.");
} 

else {

window.location = '/apex/tci_csmrg__MassEditAssets?recs='+records.join(',');
}
'''
  _parent = [
    salesforce.Asset,
  ]
}
