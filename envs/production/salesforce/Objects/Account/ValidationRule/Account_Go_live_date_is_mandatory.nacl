salesforce.ValidationRule Account_Go_live_date_is_mandatory {
  fullName = "Account.Go_live_date_is_mandatory"
  active = true
  errorConditionFormula = "ISPICKVAL( Monday_Account_Lifecycle__c , \"Onboarding\") &&  ISBLANK( Go_live_date__c )  &&  $Profile.Name ='CSM'"
  errorDisplayField = "Go_live_date__c"
  errorMessage = "When the account is under Onboarding stage you must fill 'Go Live Date'"
  _parent = [
    salesforce.Account,
  ]
}
