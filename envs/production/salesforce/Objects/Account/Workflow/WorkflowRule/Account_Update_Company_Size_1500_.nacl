salesforce.WorkflowRule Account_Update_Company_Size_1500_ {
  fullName = "Account.Update Company Size 1500%2B"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Account_Update_Company_Size_1500
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = "Long_Account_ID__c <> '0011t00000Lup6fAAB' && Long_Account_ID__c <> '0011t00000cRhxiAAC' && (( NumberOfEmployees > 1500 ) ||  (of_Employees__c > 1500 )) &&  (text(Company_Size__c) = '')"
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Account,
  ]
}
