salesforce.WorkflowRule Account_New_DM_for_account {
  fullName = "Account.New DM for account"
  actions = [
    {
      name = salesforce.WorkflowAlert.instance.Account_Titles_have_been_added_to_this_account_Account_Level
      type = "Alert"
    },
  ]
  active = false
  formula = "ISCHANGED(Number_of_titles_added__c) && Number_of_titles_added__c > 1"
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Account,
  ]
}
