salesforce.WorkflowRule Account_GSI_added_to_an_account {
  fullName = "Account.GSI added to an account"
  actions = [
    {
      name = salesforce.WorkflowAlert.instance.Account_GSI_added_to_an_account
      type = "Alert"
    },
  ]
  active = true
  criteriaItems = [
    {
      field = salesforce.Account.field.GSI_user__c
      operation = "notEqual"
    },
  ]
  triggerType = "onCreateOrTriggeringUpdate"
  _parent = [
    salesforce.Account,
  ]
}
