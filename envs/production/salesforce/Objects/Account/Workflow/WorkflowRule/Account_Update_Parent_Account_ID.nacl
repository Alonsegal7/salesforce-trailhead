salesforce.WorkflowRule Account_Update_Parent_Account_ID {
  fullName = "Account.Update Parent Account ID"
  active = false
  description = "Update Parent Account Id for Monday Account with Company but without Parent Account"
  formula = "NOT(ISBLANK(Company__c )) && ISBLANK( ParentId )&&  RecordType.DeveloperName = 'Monday_Account'"
  triggerType = "onCreateOrTriggeringUpdate"
  _parent = [
    salesforce.Account,
  ]
}
