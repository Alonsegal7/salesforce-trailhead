salesforce.WorkflowRule Lead_Lead_owner_is_SDR_BDR {
  fullName = "Lead.Lead owner is SDR%2FBDR"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Lead_SDR_name_for_reports
      type = "FieldUpdate"
    },
  ]
  active = false
  formula = "Owner:User.Profile.Name = 'SDR / BDR'"
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Lead,
  ]
}
