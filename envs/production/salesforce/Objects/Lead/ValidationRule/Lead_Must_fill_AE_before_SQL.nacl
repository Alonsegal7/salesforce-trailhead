salesforce.ValidationRule Lead_Must_fill_AE_before_SQL {
  fullName = "Lead.Must_fill_AE_before_SQL"
  active = true
  errorConditionFormula = "ISBLANK( AE__c ) &&  SQL__c =true &&  Owner:User.Profile.Name='SDR'"
  errorDisplayField = "AE__c"
  errorMessage = "You must assign AE before you mark this lead as 'SQL'"
  _parent = [
    salesforce.Lead,
  ]
}
