salesforce.WorkflowRule Contact_Mark_Contact_as_Survey_Contact {
  fullName = "Contact.Mark Contact as Survey Contact"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Contact_Set_Survey_Contact
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = '''
(isnew()||ischanged( Role_in_Monday_Account__c ))&&
ispickval(Role_in_Monday_Account__c,'Champion')
'''
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Contact,
  ]
}
