salesforce.WorkflowRule Opportunity_Update_Next_Strategic_Step_Histoey {
  fullName = "Opportunity.Update Next Strategic Step Histoey"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Opportunity_Update_Next_Strategic_Step_History
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = "NOT(ISBLANK( NextStep )) && ISCHANGED( NextStep  )"
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Opportunity,
  ]
}
