salesforce.WorkflowRule Opportunity_type_is_new_business {
  fullName = "Opportunity.type is new  business"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Opportunity_type
      type = "FieldUpdate"
    },
  ]
  active = false
  criteriaItems = [
    {
      field = salesforce.Opportunity.field.Mapped_type__c
      operation = "notEqual"
    },
  ]
  triggerType = "onCreateOrTriggeringUpdate"
  _parent = [
    salesforce.Opportunity,
  ]
}
