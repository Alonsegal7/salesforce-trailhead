salesforce.WorkflowRule Opportunity_First_Handover_Timestamp {
  fullName = "Opportunity.First Handover Timestamp"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Opportunity_Set_first_handover_timestamp_to_now
      type = "FieldUpdate"
    },
  ]
  active = true
  formula = "Handed_Over__c = True &&  (isblank(Handover_Timestamp__c ) || isnull(Handover_Timestamp__c))"
  triggerType = "onCreateOrTriggeringUpdate"
  _parent = [
    salesforce.Opportunity,
  ]
}
