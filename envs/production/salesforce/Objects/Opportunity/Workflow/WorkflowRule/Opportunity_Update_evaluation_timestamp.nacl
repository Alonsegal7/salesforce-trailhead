salesforce.WorkflowRule Opportunity_Update_evaluation_timestamp {
  fullName = "Opportunity.Update evaluation timestamp"
  actions = [
    {
      name = salesforce.WorkflowFieldUpdate.instance.Opportunity_Update_evaluation_timestamp
      type = "FieldUpdate"
    },
  ]
  active = true
  booleanFilter = "1 AND 2"
  criteriaItems = [
    {
      field = salesforce.Opportunity.field.Evaluation_timestamp__c
      operation = "equals"
    },
    {
      field = salesforce.Opportunity.field.Buying_process_timestamp__c
      operation = "notEqual"
    },
  ]
  triggerType = "onAllChanges"
  _parent = [
    salesforce.Opportunity,
  ]
}
