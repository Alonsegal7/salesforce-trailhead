salesforce.ListView Opportunity_AllOpportunities {
  fullName = "Opportunity.AllOpportunities"
  columns = [
    "OPPORTUNITY.NAME",
    "ACCOUNT.NAME",
    "CORE.USERS.FULL_NAME",
    "OPPORTUNITY.CLOSE_DATE",
    "OPPORTUNITY.TYPE",
    "OPPORTUNITY.STAGE_NAME",
    "OPPORTUNITY.PROBABILITY",
    "Expected_Plan_Name__c",
    "Expected_ARR__c",
    "Green_Bucket_ARR__c",
    "Forecast_ARR__c",
    "primary_pulse_account_id__c",
    "Sale__c",
    "OPPORTUNITY.CREATED_DATE",
  ]
  filterScope = "Everything"
  filters = [
    {
      field = "OPPORTUNITY.RECORDTYPE"
      operation = "equals"
      value = "Opportunity.Internal_Opportunity"
    },
    {
      field = "CORE.USERS.FULL_NAME"
      operation = "contains"
      value = "Maayan"
    },
  ]
  label = "All Sales Opportunities"
  sharedTo = {
    roleAndSubordinatesInternal = "Head_of_Sales"
  }
  _parent = [
    salesforce.Opportunity,
  ]
}
