type salesforce.Activity {
  salesforce.Text aircall__Answered_by__c {
    apiName = "Activity.aircall__Answered_by__c"
    description = "Aircall user who answered the call."
    externalId = false
    label = "Answered by"
    length = 255
    _required = false
    unique = false
  }
  salesforce.Url aircall__Call_Recording__c {
    apiName = "Activity.aircall__Call_Recording__c"
    description = "Aircall call recording"
    label = "Call Recording"
    _required = false
  }
  salesforce.FormulaText aircall__Connection_status__c {
    apiName = "Activity.aircall__Connection_status__c"
    externalId = false
    formula = "IF( aircall__Has_connected__c , 'Connected', 'Not connected')"
    label = "Connection status"
    _required = false
    unique = false
  }
  salesforce.Text aircall__Country__c {
    apiName = "Activity.aircall__Country__c"
    externalId = false
    label = "Country"
    length = 255
    _required = false
    unique = false
  }
  salesforce.FormulaText aircall__Detailed_type__c {
    apiName = "Activity.aircall__Detailed_type__c"
    externalId = false
    formula = '''
IF(CONTAINS(LOWER(Subject), 'missed'), 'Missed call',
 IF(CONTAINS(LOWER(Subject), 'voicemail'), 'Voicemail',
 IF(CONTAINS(LOWER(Subject), 'outbound unanswered'), 'Outbound unanswered',
 IF(CONTAINS(LOWER(Subject), 'outbound'), 'Outbound answered',
 IF(CONTAINS(LOWER(Subject), 'inbound'), 'Inbound answered',
  TEXT(CallType) 
)))))
'''
    label = "Detailed type"
    _required = false
    unique = false
  }
  salesforce.FormulaCheckbox aircall__Has_connected__c {
    apiName = "Activity.aircall__Has_connected__c"
    formula = "ISPICKVAL(CallType, 'Inbound') &&  !aircall__Is_Missed_call__c &&  !aircall__Is_Voicemail__c"
    label = "Has connected"
  }
  salesforce.FormulaText aircall__Hour_of_the_day__c {
    apiName = "Activity.aircall__Hour_of_the_day__c"
    externalId = false
    formula = "LEFT(TEXT(TIMEVALUE(DATETIMEVALUE(CreatedDate) + aircall__Timezone__c/24)), 2)+':00'"
    formulaTreatBlanksAs = "BlankAsZero"
    label = "Hour of the day"
    _required = false
    unique = false
  }
  salesforce.FormulaCheckbox aircall__Is_Missed_call__c {
    apiName = "Activity.aircall__Is_Missed_call__c"
    formula = "BEGINS(LOWER(Subject), 'missed call')"
    formulaTreatBlanksAs = "BlankAsZero"
    label = "Is Missed call"
  }
  salesforce.FormulaCheckbox aircall__Is_Voicemail__c {
    apiName = "Activity.aircall__Is_Voicemail__c"
    formula = "BEGINS('voicemail', LOWER(Subject))"
    label = "Is Voicemail"
  }
  salesforce.FormulaText aircall__Missed_Call_Ratio__c {
    apiName = "Activity.aircall__Missed_Call_Ratio__c"
    externalId = false
    formula = '''
IF(CONTAINS(CallDisposition, "out_of_opening_hours"), "Out of opening hours", 
IF(CONTAINS(CallDisposition, "no_available_agent"), "No available agent", 
IF(CONTAINS(CallDisposition, "abandoned_in_ivr"), "Abandoned in IVR", 
IF(CONTAINS(CallDisposition, "short_abandoned"), "Short abandoned", 
IF(CONTAINS(CallDisposition, "agents_did_not_answer"), "Agents did not response", NULL)))))
'''
    formulaTreatBlanksAs = "BlankAsZero"
    label = "Missed Call Ratio"
    _required = false
    unique = false
  }
  salesforce.Phone aircall__Phone_number__c {
    apiName = "Activity.aircall__Phone_number__c"
    label = "Aircall number"
    _required = false
  }
  salesforce.FormulaNumber aircall__Timezone__c {
    apiName = "Activity.aircall__Timezone__c"
    externalId = false
    formula = '''
(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 00:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 01:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 02:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 03:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 04:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 05:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 06:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 07:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 08:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 09:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 10:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 11:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 12:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 13:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 14:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 15:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 16:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 17:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 18:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 19:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 20:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 21:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 22:00:00")) - TODAY()) + 

(DATEVALUE(DATETIMEVALUE(TEXT(TODAY())+" 23:00:00")) - TODAY())
'''
    formulaTreatBlanksAs = "BlankAsZero"
    label = "Timezone"
    precision = 18
    _required = false
    scale = 0
    unique = false
  }
  salesforce.Text aircall__Transferred_to__c {
    apiName = "Activity.aircall__Transferred_to__c"
    description = "Aircall user to whom the call was transferred."
    externalId = false
    label = "Transferred to"
    length = 255
    _required = false
    unique = false
  }
  salesforce.Number aircall__Waiting_Time__c {
    apiName = "Activity.aircall__Waiting_Time__c"
    description = "Time during which the caller waited in line before being connected with the callee."
    externalId = false
    label = "Waiting Time"
    precision = 18
    _required = false
    scale = 0
    unique = false
  }
}
